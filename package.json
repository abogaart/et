{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "name": "@bloomreach/et",
  "description": "Bloomreach Engineering Tooling",
  "version": "0.0.0",
  "author": "Arthur Bogaart @abogaart",
  "bin": {
    "et": "./bin/run"
  },
  "bugs": "https://github.com/abogaart/et/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/abogaart/et",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "et",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "abogaart/et",
  "scripts": {
    "compile": "tsc -b --clean && tsc -b",
    "compile:watch": "tsc -b --watch",
    "lint": "tslint -p test -t stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "test:ci": "jest --coverage --coverage-reporters=lcov",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
